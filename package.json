{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "bootstrap": "^4.5.2",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.50",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-manifest": "^2.4.24",
    "gatsby-plugin-offline": "^3.2.24",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.14",
    "gatsby-plugin-sharp": "^2.6.28",
    "gatsby-plugin-typescript": "^2.4.19",
    "gatsby-source-contentful": "^2.3.46",
    "gatsby-source-filesystem": "^2.3.25",
    "gatsby-transformer-sharp": "^2.5.13",
    "lodash": "^4.17.20",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^11.0.2",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-preset-gatsby": "^0.5.9",
    "eslint": "^7.10.0",
    "eslint-plugin-typescript": "^0.14.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "prettier": "2.1.0",
    "react-test-renderer": "^16.13.1",
    "standard": "^14.3.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "codestyle-check": "standard --parser babel-eslint \"src/**/*.js\" \"__tests__/**/*.js\"",
    "codestyle-check-typescript": "standard --parser @typescript-eslint/parser --plugin typescript \"src/**/*.ts\" \"src/**/*.tsx\" \"__tests__/**/*.ts\" \"__tests__/**/*.tsx\"",
    "codestyle-fix": "standard --parser babel-eslint \"src/**/*.js\" \"__tests__/**/*.js\" --fix",
    "codestyle-typescript-fix": "standard --parser @typescript-eslint/parser --plugin typescript \"src/**/*.ts\" \"src/**/*.tsx\" \"__tests__/**/*.ts\" \"__tests__/**/*.tsx\" --fix",
    "build": "gatsby build",
    "develop": "gatsby clean && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
